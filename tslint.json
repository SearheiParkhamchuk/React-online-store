{
    "extends": ["tslint-config-airbnb", "tslint-react"],
    "rules": {
      "ter-indent": [true, "tab"],
      "indent": [true, "tabs", 2],
      "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
      "quotemark": [true, "single", "jsx-single"],
      "jsx-no-multiline-js": false,
      "jsx-no-lambda": false
    }
  }